<%- include("partials/header") %>


<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
        <label>Title</label> <br>
        <input type="text" name="postTitle" class="form-control"><br>
        <label>Post</label> <br>
        <textarea name="postBody" cols="30" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" name="name" class="btn btn-primary">Publish</button>

</form>


