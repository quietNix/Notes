<%- include("partials/header") %>
<% posts.forEach(function(post){ %>
<h2><%= post.title %> </h2>
<p><%= post.content.substring(0,120) %> <a href="/posts/<%=user%>/<%= post._id %> ">... Read More</a></p>
<% }); %>

<button id="Submit"><a href="/compose/<%=user%>">Compose</a></button>

